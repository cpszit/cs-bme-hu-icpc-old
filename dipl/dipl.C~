#include <stdio.h>
#include <string.h>

int main()
{
  int adj[10][10];
  char line[10][200];
  char lang[10][10];
  int i,j,k;
  char *p,*q;
  int lang[256];
  for (i=0;i<10;i++)
    {
      for (k=0;k<10;k++) adj[i][j]=0;
      fgets(line[i]);
    }
  for (i=0;i<10;i++)
    {
      for (j=0;j<10;j++)
	{
	  p=strchr(line[i]+1,' ');
	  for (k=0;k<256;k++)lang[k]=0;
	  for (;*p!=' ';p++) lang[*p]=1;
	  q=strchr(line[j]+1,' ');
	  for (;*q!=' ';q++)
	    {
	      if (lang[*q]) break;
	    }
	  if (*q!=' ') break;
	  char country[4];
	  strncpy(country,line[j],3);
	  country[3]=0;
	  if (strstr(p,country)) adj[i][j]=1;
	}
    }
  
}
